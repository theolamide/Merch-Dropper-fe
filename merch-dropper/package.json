    "@cypress/code-coverage": "^3.8.1",
    "@fortawesome/fontawesome-free": "^5.13.0",
    "@material-ui/core": "^4.9.10",
    "@testing-library/jest-dom": "^5.5.0",
    "@testing-library/react": "^10.0.2",
    "@testing-library/user-event": "^10.0.1",
    "auth0-lock": "^11.23.0",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "classnames": "^2.2.6",
    "cloudinary": "^1.21.0",
    "core-js": "^3.6.5",
    "cypress": "^4.7.0",
    "env-cmd": "^10.1.0",
    "istanbul-lib-coverage": "^3.0.0",
    "nyc": "^15.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^6.1.1",
    "react-test-renderer": "^16.13.1",
    "reactstrap": "^8.4.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5",
    "reselect": "^4.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.88.2",
    "stripe": "^8.40.0",
    "styled-components": "^5.1.0"
  },
  "scripts": {
    "start": "npm-run-all start-dev start-mockapi",
    "start-dev": "env-cmd react-scripts -r @cypress/instrument-cra start",
    "start-mockapi": "json-server ...",
    "cypress": "cypress open",
    "dev": "start-test 3000 cypress",
    "client": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --watchAll",
    "eject": "react-scripts eject",
    "coverage": "jest --coverage --detectOpenHandles --forceExit || true",
    "cyp": "node_modules/.bin/cypress open"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@cypress/instrument-cra": "^1.1.1",
    "cypress": "^4.7.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-enzyme": "^7.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-styled-components": "^7.0.2"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|sass|scss)$": "identity-obj-proxy",
      "\\.(svg|png)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/__tests__/**"
    ],
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/jest-transformer.js",
      "^.+\\.(svg|png)$": "<rootDir>/svgTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ]
  }
}
